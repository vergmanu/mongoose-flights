<%- include('../partials/header') %>

  <table>
    <thead>
      <tr>
        <th>airline</th>
        <th>airport</th>
        <th>flightNo</th>
        <th>departs</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td> 
          <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
        </tr>
      <%  }) %>
    </tbody>
  </table>
  
  <%- include('../partials/footer') %>